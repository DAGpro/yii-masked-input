{
    "name": "yiisoft/yii2-maskedinput",
    "description": "Masked input extension for the Yii framework",
    "keywords": ["yii2", "masked input", "inputmask"],
    "type": "yii2-extension",
    "license": "BSD-3-Clause",
    "support": {
        "issues": "https://github.com/yiisoft/yii2-maskedinput/issues",
        "forum": "http://www.yiiframework.com/forum/",
        "wiki": "http://www.yiiframework.com/wiki/",
        "irc": "irc://irc.freenode.net/yii",
        "source": "https://github.com/yiisoft/yii2-maskedinput"
    },
    "authors": [
        {
            "name": "Kartik Visweswaran",
            "email": "kartikv2@gmail.com"
        },
        {
            "name": "Alexander Makarov",
            "email": "sam@rmcreative.ru"
        }
    ],
    "require": {
        "yiisoft/yii2": "2.1.x-dev",
        "bower-asset/inputmask": "~3.3.5"
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://asset-packagist.org"
        }
    ],
    "autoload": {
        "psr-4": {
            "yii\\widgets\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {"yiiunit\\maskedinput\\": "tests/"}
    },
    "extra": {
        "branch-alias": {
            "dev-master": "2.1.x-dev"
        },
        "asset-installer-paths": {
            "npm-asset-library": "vendor/npm",
            "bower-asset-library": "vendor/bower"
        }
    }
}
